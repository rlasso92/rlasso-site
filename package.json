{
  "name": "rlasso-site",
  "private": true,
  "description": "this site describe the rlasso portafolio",
  "version": "0.1.0",
  "main": "./src/App.tsx",
  "license": "MIT",
  "scripts": {
    "build:ts": "gatsby build",
    "develop": "gatsby develop",
    "predev": "yarn build:ts",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:check": "prettier --check ./src/**/*.{js,ts,tsx}",
    "prettier:fix": "prettier --write ./src/**/*.{js,ts,tsx}",
    "lint": "npm run lint:check",
    "lint:check": "eslint ./src/**/*.{js,ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{js,ts,tsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:ci": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "docker:build": "docker build -t rlasso-site:lastest .",
    "docker:run": "docker run -it --init --rm --name rlasso-site -d -p 8080:8080 -t rlasso-site:lastest"
  },
  "dependencies": {
    "gatsby": "^3.13.0",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.14.3",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-sharp": "^2.12.0",
    "joi": "^17.4.0",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primereact": "^6.0.2",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "concurrently": "^5.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-transition-group": "^4.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rlasso92/rlasso-site.git"
  },
  "bugs": {
    "url": "https://github.com/rlasso92/rlasso-site/issues"
  }
}
